    if (start < 0) { return new _TYPE[] { }; }
    if (end > seq.length-1) { return new _TYPE[] { }; }
    if (sliceStart > sliceEnd) { return new _TYPE[] { }; }
    int length = sliceEnd - sliceStart + 1;
    return utilMDE.ArraysMDE.subarray(seq, sliceStart, length);
  }

